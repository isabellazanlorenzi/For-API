$(document).ready(function(){
if ($(".colorbox")[0]){
$(".colorbox").colorbox({maxWidth: "90%", maxHeight: "90%", onComplete: function(e){}});
}
});
function showImgPopup(id) {
$("a[id='" + id + "']").colorbox({open:true});
}
</script>
